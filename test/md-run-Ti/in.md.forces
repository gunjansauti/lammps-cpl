echo both
units		metal
atom_style	atomic

neighbor	0.3 bin
neigh_modify	 every 2 delay 10 check yes

read_data       omega_Ti.dat

mass		1 47.867

group		Al type 1

pair_style 	pace
pair_coeff  * * Ti.pbe.ace Ti


fix 1 all box/relax iso 0.0 vmax 0.001
min_style	cg
minimize	1.0e-10 1.0e-10 1000 1000

velocity        all create 600.03 8728
timestep        0.0005
fix		2 all nve

thermo 		1
thermo_style 	custom step etotal pe ke pxx pyy pzz press temp vol
print "Run NVE for 1000 steps"
run		1000
print "DONE"

